<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.chip.Chip xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/Widget.MaterialComponents.Chip.Entry"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:checkable="false"
    android:ellipsize="middle"
    android:maxWidth="220dp"
    android:textColor="?android:textColorPrimary"
    app:checkedIconEnabled="false"
    app:checkedIconVisible="false"
    app:chipBackgroundColor="?attr/colorPrimaryInverse"
    app:chipIconSize="32dp"
    app:chipMinHeight="36dp"
    app:chipStrokeColor="?android:textColorSecondary"
    app:chipStrokeWidth="1dp"
    app:closeIconStartPadding="0dp"
    app:closeIconTint="?android:textColorPrimary"
    app:iconEndPadding="0dp"
    app:iconStartPadding="4dp"
    app:textEndPadding="4dp" />